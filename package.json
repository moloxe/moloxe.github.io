{
  "name": "x-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently --kill-others --success=first \"bun --watch local/blog-data-server/index.ts --dev\" \"astro dev\"",
    "build": "concurrently --kill-others --success=first \"bun --watch local/blog-data-server/index.ts --dev\" \"astro check && astro build\"",
    "check": "concurrently --kill-others --success=first \"bun --watch local/blog-data-server/index.ts --dev\" \"astro check\"",
    "deploy": "sh deploy.sh",
    "start": "astro dev",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/react": "4.3.0",
    "@astrojs/tailwind": "6.0.2",
    "@astrojs/vue": "5.1.0",
    "astro": "5.12.8",
    "fuse.js": "7.0.0",
    "highlight.js": "11.9.0",
    "marked": "12.0.2",
    "marked-highlight": "2.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "vue": "3.4.24"
  },
  "devDependencies": {
    "@elysiajs/cors": "1.3.3",
    "@types/node": "20.12.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@webgpu/types": "0.1.64",
    "concurrently": "8.2.2",
    "elysia": "1.0.14"
  }
}
